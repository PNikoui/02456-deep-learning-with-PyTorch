    "n_out = 1\n",
    "tanh = np.sqrt(2/(n_in+n_out))\n",
    "Sigmoid = 4*np.sqrt(2/(n_in+n_out))\n",
    "ReLU = np.sqrt(2)*np.sqrt(2/(n_in+n_out))"
